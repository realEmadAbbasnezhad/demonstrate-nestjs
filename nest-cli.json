{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/demonstrate-nestjs/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/demonstrate-nestjs/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/demonstrate-nestjs",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "catalog": {
      "type": "application",
      "root": "apps/catalog",
      "entryFile": "main",
      "sourceRoot": "apps/catalog/src",
      "compilerOptions": {
        "tsConfigPath": "apps/catalog/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "contracts": {
      "type": "library",
      "root": "libs/contracts",
      "entryFile": "index",
      "sourceRoot": "libs/contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/contracts/tsconfig.lib.json"
      }
    },
    "demonstrate-nestjs": {
      "type": "application",
      "root": "apps/demonstrate-nestjs",
      "entryFile": "main",
      "sourceRoot": "apps/demonstrate-nestjs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/demonstrate-nestjs/tsconfig.app.json"
      }
    },
    "gateway-rest": {
      "type": "application",
      "root": "apps/gateway-rest",
      "entryFile": "main",
      "sourceRoot": "apps/gateway-rest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway-rest/tsconfig.app.json"
      }
    },
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json"
      }
    }
  }
}